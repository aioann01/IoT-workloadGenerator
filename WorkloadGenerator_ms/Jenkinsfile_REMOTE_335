   node('master') {
	stage('Checkout'){
		echo 'Checkout..'
	    	git 'https://github.com/aioann01/IoT-workloadGenerator.git'
	}
        stage('Build') {
                echo 'Building..'
		sh 'cd WorkloadGenerator_ms/'
		sh 'mvn clean install'		
        }
        stage('Test') {  
		echo 'Testing..'
	}
        stage('Deploy') {
                echo 'Deploying....'
        }
   }
