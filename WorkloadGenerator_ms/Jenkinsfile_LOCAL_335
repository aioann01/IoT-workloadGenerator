pipeline {
    agent any

    stages {
	
		stage('Checkout'){
		    echo 'Checkout..'
			git 'https://github.com/aioann01/IoT-workloadGenerator.git'
			cd WorkloadGenerator_ms
			}
        stage('Build') {
            steps {
                echo 'Building..'
				mvn clean install
				
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}