   node('master') {
	stage('Checkout'){
		echo 'Checkout..'
	    	git 'https://github.com/aioann01/IoT-workloadGenerator.git'
		bat 'cd WorkloadGenerator_ms'
	}
        stage('Build') {
                echo 'Building..'
		bat 'mvn clean install'		
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
                echo 'Deploying....'
        }
   }
